define fun1 (n) {
    return 1/n;
}; 
define gun1 (n) {
    return s(n);
};
define convolve (k) {
    sum = 0.0;
    for (i = 1; i*i <= k; i++) {
        if (k % i == 0) {
            sum += (fun1(i) * gun1(k/i));
            if (i*i != k) sum += (fun1(k/i) * gun1(i));
        }
    }
    return sum;
};

