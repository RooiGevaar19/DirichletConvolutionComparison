@use(Math)
@use(Array)

function { 2 ^ } -> f
function { sin } -> g

function {
	>k >g >f
	0 -> sum
	[ 1 1 $k sqrt seq ] ( $k swap mod 0 = ) filter -> divisors
	for ( $i : $divisors ) {
		$i f $k $i div g * $sum + -> sum
		if ( $i $i * $k != ) { 
			$i g $k $i div f * $sum + -> sum
		}
	}
	$sum
} -> DirichletConvolve

$f $g 4 DirichletConvolve
$f $g 250 DirichletConvolve
$f $g 10000 DirichletConvolve
